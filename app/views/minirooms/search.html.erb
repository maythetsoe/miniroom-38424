<%= form_with(url: search_minirooms_path, local: true, method: :get, class: "search-form") do |form| %>
  <%= form.text_field :keyword, placeholder: "投稿を検索する", class: "search-input" %>
  <%= form.submit "検索", class: "search-btn" %>
<% end %>
<div class="contents row">
  <% @minirooms.each do |miniroom| %>
    <%= render partial: "miniroom", locals: { miniroom: miniroom } %>
  <% end %>
</div>